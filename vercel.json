{
  "version": 2,
  "builds": [
    { "src": "backend/index.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/api/animalitos-hourly",
      "dest": "backend/index.js"
    },
    {
      "src": "/",
      "dest": "backend/index.js"
    }
  ],
  "functions": {
    "backend/index.js": {
      "memory": 1024,
      "maxDuration": 20
    }
  }
}
